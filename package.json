{
  "private": true,
  "name": "turbo-vite",
  "version": "0.0.0",
  "license": "UNLICENSE",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier . --check --loglevel=warn",
    "format:fix": "prettier . --write",
    "reset": "bash ./scripts/reset.sh"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "prettier-config-custom": "*",
    "turbo": "latest"
  },
  "packageManager": "npm@9.6.4"
}
