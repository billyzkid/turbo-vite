{
  "private": true,
  "name": "turbo-vite",
  "version": "0.0.0",
  "license": "UNLICENSE",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --no-daemon",
    "build": "turbo run build --no-daemon",
    "preview": "turbo run preview --no-daemon",
    "clean": "turbo run clean --no-daemon",
    "format": "prettier . --check --loglevel=warn",
    "format:fix": "prettier . --write",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "reset": "bash ./scripts/reset.sh"
  },
  "devDependencies": {
    "@turbo-vite/eslint-config": "*",
    "@turbo-vite/prettier-config": "*",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "turbo": "^1.10.2-canary.1"
  },
  "packageManager": "npm@9.6.4"
}
