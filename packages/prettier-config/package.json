{
  "private": true,
  "name": "prettier-config",
  "version": "0.0.0",
  "main": "./index.js",
  "scripts": {
    "format": "prettier . --check --ignore-path ../../.prettierignore --loglevel=warn",
    "format:fix": "prettier . --write --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rimraf ./.turbo"
  },
  "devDependencies": {
    "eslint": "latest",
    "prettier": "latest",
    "rimraf": "latest"
  }
}
