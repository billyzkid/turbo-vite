  "dependencies": {
    "ui": "*"
  },
  "devDependencies": {
    "eslint": "^8.42.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0-alpha.12",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3",
    "typescript-config": "*",
    "vite": "^4.3.9"
  }
}
