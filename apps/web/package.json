{
  "private": true,
  "name": "web",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prebuild": "npm-run-all clean format lint typecheck",
    "clean": "rimraf ./dist",
    "format": "prettier . --check --ignore-path ../../.prettierignore --loglevel=warn",
    "format:fix": "prettier . --write --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ui": "*"
  },
  "devDependencies": {
    "eslint": "latest",
    "npm-run-all": "latest",
    "prettier": "latest",
    "rimraf": "latest",
    "typescript": "5.0.4",
    "vite": "latest"
  }
}
