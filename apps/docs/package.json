    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ui": "*"
  },
  "devDependencies": {
    "eslint": "*",
    "npm-run-all": "*",
    "prettier": "*",
    "rimraf": "*",
    "typescript": "*",
    "typescript-config": "*",
    "vite": "*"
  }
}
