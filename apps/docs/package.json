{
  "private": true,
  "name": "docs",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prebuild": "npm-run-all clean format lint typecheck",
